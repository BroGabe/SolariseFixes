<?xml version="1.0" encoding="UTF-8"?>
<project xmlns="http://maven.apache.org/POM/4.0.0" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://maven.apache.org/POM/4.0.0 http://maven.apache.org/maven-v4_0_0.xsd">
  <modelVersion>4.0.0</modelVersion>
  <groupId>com.thedev</groupId>
  <artifactId>SolariseFixes</artifactId>
  <name>SolariseFixes</name>
  <version>1.0-SNAPSHOT</version>
  <build>
    <resources>
      <resource>
        <filtering>true</filtering>
        <directory>src/main/resources</directory>
      </resource>
    </resources>
    <plugins>
      <plugin>
        <artifactId>maven-compiler-plugin</artifactId>
        <version>3.8.1</version>
        <configuration>
          <source>${java.version}</source>
          <target>${java.version}</target>
        </configuration>
      </plugin>
      <plugin>
        <artifactId>maven-shade-plugin</artifactId>
        <version>3.2.4</version>
        <executions>
          <execution>
            <phase>package</phase>
            <goals>
              <goal>shade</goal>
            </goals>
          </execution>
        </executions>
      </plugin>
    </plugins>
  </build>
  <repositories>
    <repository>
      <id>spigotmc-repo</id>
      <url>https://hub.spigotmc.org/nexus/content/repositories/snapshots/</url>
    </repository>
    <repository>
      <id>sonatype</id>
      <url>https://oss.sonatype.org/content/groups/public/</url>
    </repository>
    <repository>
      <id>essentials-releases</id>
      <url>https://repo.essentialsx.net/releases/</url>
    </repository>
  </repositories>
  <dependencies>
    <dependency>
      <groupId>org.spigotmc</groupId>
      <artifactId>spigot-api</artifactId>
      <version>1.8.8-R0.1-SNAPSHOT</version>
      <scope>provided</scope>
    </dependency>
    <dependency>
      <groupId>com.golfing8.factions</groupId>
      <artifactId>FactionsFork</artifactId>
      <version>1.0</version>
      <scope>system</scope>
      <systemPath>${project.basedir}/Libs/Factions.jar</systemPath>
    </dependency>
    <dependency>
      <groupId>com.essx.dev</groupId>
      <artifactId>EssentialsX</artifactId>
      <version>1.0</version>
      <scope>system</scope>
      <systemPath>${project.basedir}/Libs/EssentialsX.jar</systemPath>
    </dependency>
    <dependency>
      <groupId>com.manticswords.dev</groupId>
      <artifactId>ManticSwords</artifactId>
      <version>1.0</version>
      <scope>system</scope>
      <systemPath>${project.basedir}/Libs/ManticSwords.jar</systemPath>
    </dependency>
    <dependency>
      <groupId>com.mantichoes.dev</groupId>
      <artifactId>ManticHoes</artifactId>
      <version>1.0</version>
      <scope>system</scope>
      <systemPath>${project.basedir}/Libs/ManticHoes.jar</systemPath>
    </dependency>
    <dependency>
      <groupId>com.manticrods.dev</groupId>
      <artifactId>ManticRods</artifactId>
      <version>1.0</version>
      <scope>system</scope>
      <systemPath>${project.basedir}/Libs/ManticRods.jar</systemPath>
    </dependency>
    <dependency>
      <groupId>com.manticlib.dev</groupId>
      <artifactId>ManticLib</artifactId>
      <version>1.0</version>
      <scope>system</scope>
      <systemPath>${project.basedir}/Libs/ManticLib-1.0.48.4 (1).jar</systemPath>
    </dependency>
    <dependency>
      <groupId>com.fkore.dev</groupId>
      <artifactId>FactionsKore</artifactId>
      <version>1.0</version>
      <scope>system</scope>
      <systemPath>${project.basedir}/Libs/FactionsKore-1.3.67.7.jar</systemPath>
    </dependency>
    <dependency>
      <groupId>com.fkore.dev</groupId>
      <artifactId>KoreFTOP</artifactId>
      <version>1.0</version>
      <scope>system</scope>
      <systemPath>${project.basedir}/Libs/KoreFTOP.jar</systemPath>
    </dependency>
    <dependency>
      <groupId>com.lpx.dev</groupId>
      <artifactId>LPX</artifactId>
      <version>1.0</version>
      <scope>system</scope>
      <systemPath>${project.basedir}/Libs/LPX.jar</systemPath>
    </dependency>
    <dependency>
      <groupId>com.combattag.dev</groupId>
      <artifactId>CombatTagPlus</artifactId>
      <version>1.0</version>
      <scope>system</scope>
      <systemPath>${project.basedir}/Libs/CombatTagPlus.jar</systemPath>
    </dependency>
    <dependency>
      <groupId>com.winedev.dev</groupId>
      <artifactId>WineSpigot</artifactId>
      <version>1.0</version>
      <scope>system</scope>
      <systemPath>${project.basedir}/Libs/WineSpigot.jar</systemPath>
    </dependency>
  </dependencies>
  <properties>
    <java.version>1.8</java.version>
    <project.build.sourceEncoding>UTF-8</project.build.sourceEncoding>
  </properties>
</project>
